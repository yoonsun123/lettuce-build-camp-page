        }

        .more-info p, .more-info ul {
            margin-bottom: 15px;
        }

        .more-info ul {
            list-style-position: inside;
            padding-left: 10px;
        }

        .more-info li {
            margin-bottom: 5px;
        }

        @media (max-width: 768px) {
            body { padding-top: 70px; }
            nav { height: 70px; padding: 0 4%; }
            .nav-links { display: none; }
            .hero h1 { font-size: 2.5em; }
            .intro h2 { font-size: 2.2em; }
            .cta-section h2 { font-size: 2.2em; }
            .camps-grid { grid-template-columns: 1fr; }
            .camps-background-wrapper { padding: 20px; }
        }
    </style>
</head>
